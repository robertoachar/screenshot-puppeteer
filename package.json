    "esm": "3.2.25"
  },
  "devDependencies": {
    "@babel/core": "7.10.2",
    "@babel/preset-env": "7.10.2",
    "@types/jest": "25.2.3",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.0.1",
    "eslint": "7.2.0",
    "eslint-config-airbnb-base": "14.1.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.21.1",
    "eslint-plugin-prettier": "3.1.3",
    "jest": "26.0.1",
    "jest-runner-eslint": "0.9.0",
    "jest-watch-typeahead": "0.6.0",
    "prettier": "2.0.5"
